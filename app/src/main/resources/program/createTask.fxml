<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="program.controllers.CreateTaskController" prefHeight="720.0" prefWidth="1280.0">
    <VBox layoutX="30.0" layoutY="35.0" prefHeight="720.0" prefWidth="1220.0">
        <Label text="Task:" />
        <TextField fx:id="taskName" />
        <Label fx:id="promptLabel" />
        <TextField fx:id="taskDescription" />

        <Button mnemonicParsing="false" onAction="#addTask" text="Add Task"/>

        <Button fx:id="button" mnemonicParsing="false" onAction="#goBack" text="Cancel"/>
    </VBox>
</AnchorPane>